{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["standard"],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest"
	},
	"plugins": ["@typescript-eslint"],
	"rules": {
		"semi": "off",
		"no-tabs": "off",
		"indent": "off",
		"comma-dangle": "off",
		"space-before-function-paren": "off"
	}
}
